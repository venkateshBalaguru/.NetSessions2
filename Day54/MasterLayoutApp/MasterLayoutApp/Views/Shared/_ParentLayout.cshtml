<!DOCTYPE html>
    <html>
    <head>
	<title>@ViewBag.ChildTitle</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    </head>
    <body>
@*       <nav>
        @Html.ActionLink("Home","Index","Home") |
        @Html.ActionLink("About", "Index", "AboutUs") |
        @Html.ActionLink("ContactUs", "Index", "ContactUs")|
      </nav>

      <div>
          @RenderBody()    //Render the content from the child 
      </div> *@

    <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
            <a class="navbar-brand" href="/">MyBaning App</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        @Html.ActionLink("Bootrap Home", "Index", "Home", null, new { @class = "nav-link active" })
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("Bootrap ContactUs", "Index", "ContactUs", null, new { @class = "nav-link active" })
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("Bootrap AboutUs", "Index", "AboutUs", null, new { @class = "nav-link active" })
                    </li>

                </ul>
                <form class="d-flex" role="search">
                    <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-success" type="submit">Search</button>
                </form>
            </div>
        </div>
    </nav>

    <div class="container">
        @RenderBody() <!--Render child contents -->
    </div>
    <footer>
        <p> &copy;  @DateTime.Now.Year</p>
    </footer>
    </body>
    </html>